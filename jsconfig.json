{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~components/*": ["./src/components/*"],
            "~components": ["./src/components"],
            "~constants/*": ["./src/constant/*"],
            "~pages/*": ["src/pages/*"],
            "~redux/*": ["./src/redux/*"],
            "~utils/*": ["./src/utils/*"],
            "~api/*": ["./src/api/*"],
            "~/*": ["src/*"]
        },
        
    },
    "include": ["src/**/*.js", "src/**/*.jsx", "src/**/index.js"],
    "exclude": ["node_modules", "dist"]
}
